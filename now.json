{
  "version": 2,
  "name": "blog.mudit.xyz",
  "scope": "zdx",
  "alias": ["blog.mudit.xyz", "www.blog.mudit.xyz"],
  "public": false,
  "routes": [
    {
      "src": "/posts/?",
      "status": 301,
      "headers": {
        "Location": "/"
      },
      "continue": false
    }
  ],
  "build": {
    "env": {
      "CDA_TOKEN": "@blog-cda-token",
      "CMA_TOKEN": "@blog-cma-token",
      "PREVIEW_TOKEN": "@blog-preview-token",
      "SPACE_ID": "pe315guv55pz"
    }
  },
  "github": {
    "enabled": true
  }
}
