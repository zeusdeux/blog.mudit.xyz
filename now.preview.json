{
  "version": 2,
  "name": "preview.blog.mudit.xyz",
  "scope": "zdx",
  "alias": ["preview.blog.mudit.xyz"],
  "public": false,
  "routes": [
    {
      "src": "/posts/?",
      "status": 301,
      "headers": {
        "Location": "/"
      },
      "continue": false
    }
  ],
  "build": {
    "env": {
      "CMA_TOKEN": "@blog-cma-token",
      "PREVIEW_TOKEN": "@blog-preview-token",
      "SPACE_ID": "pe315guv55pz",
      "FORCE_PREVIEW": "true"
    }
  }
}
